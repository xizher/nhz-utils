    "build:date": "rollup --config build/build.date.js && npm run build",
    "build:watch": "rollup --config build/build.watch.js && npm run build",
    "build:random": "rollup --config build/build.random.js && npm run build",
    "build:format": "rollup --config build/build.format.js && npm run build",
    "build:object": "rollup --config build/build.object.js && npm run build",
    "build:array": "rollup --config build/build.array.js && npm run build",
    "build:throttle-debounce": "rollup --config build/build.throttle-debounce.js && npm run build",
    "build:handle": "rollup --config build/build.handle.js && npm run build",
    "build:transfor": "rollup --config build/build.transfor.js && npm run build",
    "build:generic": "rollup --config build/build.generic.js && npm run build",
    "build:url": "rollup --config build/build.url.js && npm run build",
    "build": "rollup --config build/index.js"
