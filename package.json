    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.0.1",
    "rollup": "^2.58.0",
    "rollup-jest": "^1.1.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.5",
